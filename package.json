    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.6.0",
    "mocha": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.1.0",
