    "express-validator": "^6.12.1",
    "express-winston": "^4.1.0",
    "moment": "^2.29.1",
    "on-headers": "^1.0.2",
    "pg": "^8.7.1",
    "prom-client": "^13.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "6.6.2",
    "sequelize-typescript": "^2.1.0",
    "tedis": "^0.1.12",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-syslog": "^2.4.4",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
