{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "lint": "eslint --ext js --ext jsx --ext ts --ext tsx src/",
    "build": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.6.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-table": "^7.7.0",
    "@types/react-widgets": "^4.4.0",
    "@types/react-widgets-moment": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-social": "^5.1.1",
    "i18next": "^20.2.2",
    "i18next-browser-languagedetector": "^6.1.0",
    "moment-timezone": "^0.5.33",
    "node-sass": "^6.0.1",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.15",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-table": "^7.7.0",
    "react-toastify": "^7.0.4",
    "react-widgets": "^5.2.0",
    "react-widgets-moment": "^5.0.7",
    "reactstrap": "^8.9.0",
    "typescript": "^4.3.5"
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "indent": "off",
      "@typescript-eslint/indent": [
        "warn",
        2
      ],
      "@typescript-eslint/no-empty-function": 0,
      "@typescript-eslint/ban-types": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
